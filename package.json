{
  "name": "img-mapper",
  "version": "2.0.1",
  "description": "Highlight interactive zones in images",
  "bugs": {
    "url": "https://github.com/img-mapper/img-mapper/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/img-mapper/img-mapper.git"
  },
  "license": "MIT",
  "author": "Nisharg Shah",
  "type": "commonjs",
  "scripts": {
    "build": "pnpm -r build",
    "docs:build": "pnpm --filter react-img-mapper-docs build",
    "docs:dev": "pnpm --filter react-img-mapper-docs dev",
    "docs:preview": "pnpm --filter react-img-mapper-docs preview",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "pnpm --silent lint --fix",
    "pkg:prepare": "pnpm changeset && pnpm changeset version",
    "pkg:publish": "pnpm -r build && pnpm changeset publish",
    "prepare": "husky || true",
    "script:lint": "bash -e ./scripts/lint.sh",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7"
  },
  "packageManager": "pnpm@10.18.1",
  "engines": {
    "node": ">=16.0.0",
    "npm": "please-use-pnpm",
    "pnpm": ">=10.12.0",
    "yarn": "please-use-pnpm"
  }
}


